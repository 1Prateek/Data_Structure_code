#include<bits/stdc++.h>
using namespace std;

vector<int> nextGreaterElement(vector<int> v){
    
    vector<int> nge(v.size());
    stack<int> st;
    
    for(int i=0;i<v.size();++1)
    {
        while(!st.empty() && v[i]>v[st.top()])
        {
            nge[st.top()]=1;
        }
        st.push(i);
    }
    while(!st.empty())
    {
        nge[st.pop()]=-1;
        st.pop;
    }
    return nge;
}
